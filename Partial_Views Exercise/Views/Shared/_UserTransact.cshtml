@model Partial_Views_Exercise.Models.TransactModels

@using Partial_Views_Exercise.Models;


    <table>
        <tr>
            <th>Product</th><th>Brand</th><th>Price (S$) </th>
        </tr>
      
        @{
            List<TransactModels> list = (List<TransactModels>)ViewData["transact"];
            foreach (var i in list)
            {
                 <tr>
                    <td>
                        @i.product
                    </td>
                    <td>
                        @i.brand
                    </td>
                    <td>
                        @i.price
                    </td>
                 </tr>
            }
         }
            
    </table>
